{
    "name": "honeymustard/field-factory",
    "description": "A facade library for Advanced Custom Fields",
    "homepage": "https://github.com/honeymustard/field-factory",
    "type": "library",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^6.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Solumsmo",
            "email": "adrian.solumsmo@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Honeymustard\\FieldFactory\\": "src"
        }
    },
    "scripts": {
        "test": [
            "phpcs --config-set report_width 100",
            "phpcs --standard=PSR2 --colors ./src",
            "phpunit tests --bootstrap ./vendor/autoload.php"
         ]
    }
}
